  "license": "MIT",
  "scripts": {
    "start": "npm-run-all -p -l start:*",
    "start:server": "npm-run-all -s -l server:*",
    "start:client": "cd client && yarn start",
    "start:nodemon": "nodemon --watch server/build server/build/bundle-server.js",
    "server:webpack": "cd server && yarn start",
    "server:start": "cd server/build && node bundle-server.js",
    "install:client": "cd client && yarn install",
    "install:server": "cd server && yarn install",
    "postinstall": "run-p install:*"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
